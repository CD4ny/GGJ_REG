// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./data.db"
}

model Participant {
    id String @id @unique
	team Team @relation(fields: [team_id],references: [id_team])
	team_id Int
	name String
	institution String
	date DateTime @default(now())
	phone String
	email String
	area String
	ext Boolean
}
// Team schema
model Team{
	id_team Int @id @default(autoincrement())
	name String
	desc String
	teammates  Participant[] 
}

//Participant Next
model  ParticipantNext{
 	//child data
	id String @id @unique
	name String
	//responsible data
	responsible String
 	responsibleId String	
	responsiblePhone Int
	institution String
	date DateTime @default(now())
	email String
	ext Boolean @default(false)
}
