// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./data.db"
}

model Participant {
  ci String @id @unique
	equipo String
	nombre String
	institucion String
	fecha DateTime @default(now())
	telefono Int
	correo String
	centro String
	ext Boolean
}
